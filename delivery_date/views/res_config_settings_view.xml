<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record model="ir.ui.view" id="website_customer_delivery_date_view">
			<field name="name">website.customer.delivery.date</field>
			<field name="model">res.config.settings</field>
			<field name="inherit_id" ref="website.res_config_settings_view_form" />
			<field name="arch" type="xml">
				<xpath expr="//field[@name='group_multi_currency']" position="after">
					<div class="row mt16 o_settings_container" id="cutomer_date_time_setting">
						<div class="col-xs-12 col-md-6 o_setting_box" style="margin-left:-32px; margin-top:40px;"
							 id="delivery_customer_setting">
							 <h2 style="width:900px; margin-left:-34px; margin-bottom:20px">Customer Delivery Configuration</h2>
							 <div class="o_setting_left_pane"><field name="disable_date" /></div>
							 <div class="o_setting_right_pane" style="width:450px;">
								 <label style="margin-bottom: 14px;" for="disable_date"/>
							 </div>
							 <div class="o_setting_left_pane"><field name="disable_comment"/></div>
							 <div class="o_setting_right_pane" style="width:450px;"><label for="disable_comment"/></div>
						</div>
					</div>
				</xpath>
			</field>
		</record>
    </data>
</odoo>